{
  "version": 2,
  "name": "gluwise-web",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://gluwise-api.vercel.app/api/v1"
  },
  "build": {
    "env": {
      "REACT_APP_API_URL": "https://gluwise-api.vercel.app/api/v1"
    }
  }
}

{
  "name": "api",
  "version": "1.0.0",
  "description": "GluWise API Server - Node.js + Express + TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/simpleIndex.ts",
    "dev:full": "tsx watch src/index.ts",
    "start": "node dist/simpleIndex.js",
    "start:full": "node dist/index.js",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:setup": "tsx src/scripts/setup-db.ts",
    "db:migrate": "tsx src/scripts/migrate.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@google-cloud/vision": "^4.0.2",
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.38.4",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "pino": "^8.16.2",
    "pino-http": "^8.5.1",
    "rate-limiter-flexible": "^4.0.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
